        <activity android:name=".Signup" />
        <activity android:name=".BabyDetails" />
        <activity android:name=".Vaccine" />
        <activity android:name=".Home" />
        <activity android:name=".MyProfile" />
        <activity android:name=".HealthCare" />
        <activity android:name=".Fever" />
        <activity android:name=".CommonCold" />
        <activity android:name=".StomachAche" />
        <activity android:name=".GasandAcidity" />
        <activity android:name=".Constipation" />
        <activity android:name=".Hiccups" />

    <service
        android:name=".MyFirebaseMessagingService"
        android:exported="false">
        <intent-filter>
            <action android:name="com.google.firebase.MESSAGING_EVENT" />
        </intent-filter>
    </service>
    </application>
</manifest>